{
  "name": "html-react-parser",
  "version": "5.2.7",
  "description": "HTML to React parser.",
  "author": "Mark <mark@remarkablemark.org> (https://remarkablemark.org)",
  "main": "./lib/index.js",
  "module": "./esm/index.mjs",
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./lib/index.js"
    },
    "./lib/*": {
      "import": "./esm/*.mjs",
      "default": "./lib/*.js"
    }
  },
  "scripts": {
    "benchmark": "ts-node benchmark",
    "build": "npm run build:cjs && npm run build:umd",
    "build:cjs": "tsc --project tsconfig.build.json",
    "build:umd": "rollup --config --failAfterWarnings",
    "clean": "rm -rf coverage lib dist",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:package": "publint",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "npm run lint && npm run lint:tsc && npm run test:ci && npm run clean && npm run build",
    "size-limit": "size-limit",
    "test": "jest --testPathIgnorePatterns __tests__/integration",
    "test:ci": "CI=true npm test -- --ci --colors",
    "test:esm": "npm run build:cjs && node --test __tests__/esm/*",
    "test:integration": "npm run build && jest --coverage=false --env=jsdom __tests__/integration",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remarkablemark/html-react-parser.git"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/html-react-parser/issues"
  },
  "keywords": [
    "html-react-parser",
    "html",
    "react",
    "parser",
    "dom"
  ],
  "dependencies": {
    "domhandler": "5.0.3",
    "html-dom-parser": "5.1.1",
    "react-property": "2.0.2",
    "style-to-js": "1.1.18"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/compat": "1.4.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.37.0",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.4",
    "@size-limit/preset-big-lib": "11.2.0",
    "@types/benchmark": "2.1.5",
    "@types/jest": "30.0.0",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "benchmark": "2.1.4",
    "eslint": "9.37.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-watch-typeahead": "3.0.1",
    "lint-staged": "16.2.4",
    "preact": "10.27.2",
    "publint": "0.3.14",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rollup": "4.52.4",
    "size-limit": "11.2.0",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0"
  },
  "peerDependencies": {
    "@types/react": "0.14 || 15 || 16 || 17 || 18 || 19",
    "react": "0.14 || 15 || 16 || 17 || 18 || 19"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "overrides": {
    "pretty-format": {
      "react-is": "$react"
    }
  },
  "files": [
    "/dist",
    "/esm",
    "/lib",
    "/src"
  ],
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/html-react-parser"
  },
  "license": "MIT"
}
