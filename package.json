{
  "name": "html-react-parser",
  "version": "5.2.2",
  "description": "HTML to React parser.",
  "author": "Mark <mark@remarkablemark.org> (https://remarkablemark.org)",
  "main": "./lib/index.js",
  "module": "./esm/index.mjs",
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./lib/index.js"
    },
    "./lib/*": {
      "import": "./esm/*.mjs",
      "default": "./lib/*.js"
    }
  },
  "scripts": {
    "benchmark": "ts-node benchmark",
    "build": "npm run build:cjs && npm run build:umd",
    "build:cjs": "tsc --project tsconfig.build.json",
    "build:umd": "rollup --config --failAfterWarnings",
    "clean": "rm -rf coverage lib dist",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "npm run lint && npm run lint:tsc && npm run test:ci && npm run clean && npm run build",
    "size-limit": "size-limit",
    "test": "jest --testPathIgnorePatterns __tests__/integration",
    "test:ci": "CI=true npm test -- --ci --colors",
    "test:esm": "npm run build:cjs && node --test __tests__/esm/*",
    "test:integration": "npm run build && jest --coverage=false --env=jsdom __tests__/integration",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remarkablemark/html-react-parser.git"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/html-react-parser/issues"
  },
  "keywords": [
    "html-react-parser",
    "html",
    "react",
    "parser",
    "dom"
  ],
  "dependencies": {
    "domhandler": "5.0.3",
    "html-dom-parser": "5.0.13",
    "react-property": "2.0.2",
    "style-to-js": "1.1.16"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/compat": "1.2.4",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@rollup/plugin-commonjs": "28.0.2",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@size-limit/preset-big-lib": "11.1.6",
    "@types/benchmark": "2.1.5",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/eslint-plugin": "8.18.2",
    "@typescript-eslint/parser": "8.18.2",
    "benchmark": "2.1.4",
    "eslint": "9.17.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    "lint-staged": "15.2.11",
    "preact": "10.25.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rollup": "4.29.1",
    "size-limit": "11.1.6",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  },
  "peerDependencies": {
    "@types/react": "0.14 || 15 || 16 || 17 || 18 || 19",
    "react": "0.14 || 15 || 16 || 17 || 18 || 19"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "files": [
    "/dist",
    "/esm",
    "/lib",
    "/src"
  ],
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/html-react-parser"
  },
  "license": "MIT"
}
